<Window x:Class="AdvanceCSharp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel>
            <TextBox Text="{Binding TestStudent.FirstName}"/>
            <TextBox Text="{Binding TestStudent.LastName}"/>
            <Label Name='Result'></Label>
            <Button Name="BtnResult">Click Me!</Button>
        </StackPanel>
    </Grid>
</Window>
